        <script type="text/javascript">
        $(function(){
            $('#btn_all_info').click(function(){
                $('div.triviaBlock')
                    .find('div.loader')
                        .css({'visibility': 'visible'})
                        .end()
                    .find('ul.trivia li, div.more')
                        .css({'visibility': 'hidden'})
                ;

                var now = new Date();
                $.get('/handler_info.php', {obj_type: 'film', obj_id: 452899, rnd: now.getTime()}, function(data) {
                    $('div.triviaBlock')
                        .find('div.loader')
                            .css({'visibility': 'hidden'})
                            .end()
                        .find('ul.trivia')
                            .css({'visibility': 'visible'})
                    ;

                    $('ul.trivia').html(data);
                });

                return false;
            });
        });
        </script>
