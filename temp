<script type="text/javascript">

function ShowBrandVideo()
{
	var w=750;
	var h=421.875;
	$('#spec_trailer').css({"display":"block"});
	$('#spec_trailer').css({"height":((h+20)+"px"),"width":"100%"});
	$('#spec_trailer_bg').css({"height":((h+45)+"px"),"width":"100%"}); 
	$('#fc_player_1').css({"width": w+"px"});
	$('#superb').css({"display":"none"});
	
	$('#fc_player_1').html("<a class='close' href='#' onclick='HideBrandVideo(); return false;' ></a><div id='fc_playerb66174' >Вам необходимо обновить Flash Player.</div>");
	getPlayer("http://tr.kinopoisk.ru/trailers/flv/465673/kinopoisk.ru-Bad-Teacher-66174.mp4","", w,h, true,"b66174");
	
	$('#fc_player_1 object,#fc_player_1 embed').css({
		"width":(w+"px"),
		"height":(h+"px")
	});
		
}






function HideBrandVideo()
{
	$('#spec_trailer').css({"display":"none"});
	$('#fc_player_1').html('');
	$('#superb').css({"display":"block"});
}

</script>

