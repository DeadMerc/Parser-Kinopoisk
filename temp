					$('.sequel_scroller .arrows_left').addClass('disable_left');
				if(position_x['sequel_scroller']<scroll_per_time['sequel_scroller'])
					$('.sequel_scroller  .arrows_right').addClass('disable_right');
   
   
});
</script>

